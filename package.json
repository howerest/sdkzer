{
	"name": "sdkzer",
	"version": "0.6.7",
	"description": "Implements a simple API to deal with RESTful web services (endpoints) from Javascript.",
	"keywords": [
		"sdk",
		"javascript",
		"api sdk",
		"javascript http sdk",
		"javascript restful http sdk",
		"restful http",
		"rest",
		"restful",
		"http",
		"http api",
		"http services",
		"api",
		"typescript",
		"model",
		"models",
		"business logic",	
		"data logic",
		"validation"
	],
	"licenses": [
		{
			"type": "Apache-2.0",
			"url": "http://www.apache.org/licenses/LICENSE-2.0"
		}
	],
	"contributors": [
		{
			"name": "David Valin",
			"url": "http://www.davidvalin.com",
			"email": "hola@davidvalin.com"
		}
	],
	"maintainers": [
		{
			"name": "David Valin",
			"url": "http://www.davidvalin.com",
			"email": "hola@davidvalin.com"
		}
	],
	"main": "dist/howerest.sdkzer.js",
	"typings": "dist/howerest.sdkzer.d.ts",
	"dependencies": {
		"js-webservices": "^0.1.1"
	},
	"devDependencies": {
		"@babel/polyfill": "^7.0.0",
		"@types/es6-promise": "0.0.32",
		"@types/jest": "^23.3.5",
		"@types/node": "^6.0.58",
		"awesome-typescript-loader": "^5.2.0",
		"jest": "^23.6.0",
		"ts-jest": "^23.10.4",
		"ts-loader": "^5.2.1",
		"tslint": "^4.3.1",
		"typedoc": "^0.11.1",
		"typescript": "^3.1.3",
		"uglifyjs-webpack-plugin": "^1.2.7",
		"utf8": "2.1.0",
		"webpack": "^4.12.0",
		"webpack-cli": "^3.0.8",
		"xhr-mock": "^2.4.1"
	},
	"optionalDependencies": {},
	"scripts": {
		"test": "yarn jest --coverage --collectCoverageFrom='[\"src/*.ts\"]' --verbose",
		"document": "yarn typedoc --mode file --out doc src/*.ts",
		"release": "yarn test && rm -rf dist && webpack --config webpack.conf.js && yarn document && mv doc dist && mv coverage dist"
	},
	"jest": {
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"transform": {
			"^.+\\.(ts|tsx)$": "ts-jest"
		},
		"testMatch": [
			"**/__test__/*_spec.+(ts)"
		]
	}
}
